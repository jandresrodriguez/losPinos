<h1>Listing admin_users</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Contrasena</th>
      <th>Nombre</th>
      <th>Habilitado</th>
      <th>Email</th>
      <th>Nivel</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_users.each do |admin_user| %>
      <tr>
        <td><%= admin_user.usuario %></td>
        <td><%= admin_user.contrasena %></td>
        <td><%= admin_user.nombre %></td>
        <td><%= admin_user.habilitado %></td>
        <td><%= admin_user.email %></td>
        <td><%= admin_user.nivel %></td>
        <td><%= link_to 'Show', admin_user %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(admin_user) %></td>
        <td><%= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin user', new_admin_user_path %>
