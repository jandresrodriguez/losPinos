<div id="pad-wrapper" class="new-user">
  <div class="row-fluid header">
    <h3>Ver Gasto</h3>
  </div>

  <div class="row-fluid form-wrapper">
    <!-- left column -->
    <div class="span9 with-sidebar">
        <div class="container"><p id="notice"><%= notice %></p>
<table class="table table-hover">

<p>
  <strong>Fecha:</strong>
  <%= @gasto.fecha %>
</p>

<p>
  <strong>Rubro:</strong>
  <%= @gasto.rubro %>
</p>

<p>
  <strong>Monto:</strong>
  <%= @gasto.monto %>
</p>

<p>
  <strong>Moneda:</strong>
  <%= @gasto.moneda %>
</p>

<p>
  <strong>Comentarios:</strong>
  <%= @gasto.comentarios %>
</p>
</table>
<%= link_to "Volver", gastos_path , class: "btn btn-default", style: "color: #0078d9; font-size: 11px; margin-left: 5px; float: right;" %>
      </div>
    </div>
  </div>
</div>
